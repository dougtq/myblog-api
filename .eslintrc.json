{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "warn",
            "double"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-console": [
            0
        ],
        "no-undef": [
          2
        ],
        "no-unused-vars": [
          0
        ],
        "no-useless-escape": [
          0
        ],
        "no-async-promise-executor": [
          0
        ],
        "no-unsafe-finally": [
          0
        ],
        "no-process-env": [
          0
        ],
        "keyword-spacing": [
          2
        ],
        "no-dupe-keys": [
          0
        ]
    }
}